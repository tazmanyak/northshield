on=@dclick
if !(<topobj.uid> == <src.uid>)
	src.sysmessage @,,1 Lockpick çantanizda olmalı.
	return 1
endif
ON=@TARGON_ITEM
IF (<SRC.TARG.DISTANCE> > 2)
	SRC.SYSMESSAGE @55,3,1 Sandiktan çok uzaktasiniz.
	RETURN 1
ELSEIF (<SRC.TARG.TYPE> == T_DOOR) || (<SRC.TARG.TYPE> == T_DOOR_LOCKED)
	SRC.SYSMESSAGE @55,3,1 Kapıları açamazsınız.
	RETURN 1
ELSEIF (<SRC.TARG.BASEID> == I_STRONG_BOX) //|| (<SRC.TARG.TYPE> == T_SECURE)
	SRC.SYSMESSAGE @55,3,1 Bunu açamazsiniz.
	RETURN 1
ELSEIF !(<SRC.TARG.MORE>==0515)
	SRC.SYSMESSAGE @38,3,1 Lockpickleri bunun üzerinde kullanamazsınız.
	RETURN 1
ENDIF


IF (strmatch('<argo.baseid>','i_chest_vesper1') || strmatch('<argo.baseid>','i_chest_vesper2'))
	SRC.SOUND=01FF
	SRC.SAYU * Kilidi açmaya çalişir *
	SRC.TARG.TIMER=120
	//SRC.TARG.LINK=0
	SRC.TARG.MORE=0
	SRC.TARG.COLOR=0
	SRC.TARG.TYPE=1
	SRC.SYSMESSAGE @,,1 Kilidi açtınız.
	src.gainpickingskill
	src.consume 1 i_lockpick
	RETURN 1
ENDIF

IF strmatch('<argo.baseid>','i_chest_hazine')
	if <src.lockpicking> < 100.0 // <def.scp.Locpick_TMapLevel_<argo.dtag.map.level>>
	src.sysmessage @,,1 Yeterli seviyede lockpicking bilmiyorsunuz. // Bu harita icin min skill : <def.scp.Locpick_TMapLevel_<argo.dtag.map.level>
	return 1
	elif strmatch("<argo.type>","t_container_locked")
		SRC.SOUND=01FF
		SRC.SAYU * Kilidi açmaya çalişir *
		SRC.TARG.TIMER=1200
		//SRC.TARG.LINK=0
		//SRC.TARG.MOREY 0
		SRC.TARG.COLOR=0
		SRC.TARG.TYPE=1
		SRC.SYSMESSAGE @,,1 Kilidi açtınız.
		src.consume 1 i_lockpick
		for 1 5
			serv.newnpc <def.scp.Monsters_TMapLevel_<eval <argo.tag.map.level>>>
			new.home=<new.p>
			new.homedist 3
			new.events +e_treasure_guardian
			new.removetimer <EVAL 60*20>
			//new.attack <src.uid>
		endfor
		//src.gainpickingskill
		RETURN 1
	else
		src.sysmessage @,,1 Kasayı zaten açmışsınız.
		return 1
	endif
ENDIF


if (<eval <argo.tag.chest_sea>> == 1) || (<eval <argo.tag.dungeon_chest>> == 1) || (<eval <argo.tag.dungeon_chest>> == 2) || (<eval <argo.tag.dungeon_chest>> == 3) || (<eval <argo.tag.dungeon_chest>> == 4) || (<eval <argo.tag.dungeon_chest>> == 5)
  if <src.lockpicking>	== 100.0
	SRC.SOUND=01FF
	SRC.SAYU * Kilidi açmaya çalişir *
	//src.targ.attr attr_decay
	SRC.TARG.TIMER=<eval <argo.tag.dungeon_chest_timer>>
	//SRC.TARG.LINK=0
	SRC.TARG.MORE=0
	SRC.TARG.COLOR=0
	SRC.TARG.TYPE=1
	SRC.SYSMESSAGE @,,1 Kilidi açtınız.
