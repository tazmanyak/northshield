on=@create
color 04a

on=@dclick
target Budamak istediginiz dekoratif bitkiyi seciniz.
return 1

on=@targon_item
if (<argo.type> == t_plant_base) && (<argo.baseid> == i_bowl_empty)
	if !<src.restest 1 i_flask1_empty>
		src.sys_normal Yaninizda bir adet bos flask olmak zorundadir.
		return 1
	else
		serv.newitem i_potion_plant
		new.color <argo.color>
		new.bounce
		src.consume 1 i_flask1_empty
		src.sys_normal Budadiginiz bitkiyi siseye doldurdunuz.
		argo.remove
		return 1
	endif
else
	src.sys_error Bunu budayamazsiniz.
	return 1
endif
	


on=@targon_char
src.sys_error Canli varlik budayamazsiniz.
return 1

on=@targon_ground
return 1

