[ITEMDEF i_talisman_warrior]
id=i_talisman4
NAME=Talisman of the Light
TYPE=t_jewelry
SKILLMAKE=TINKERING 100.0,MAGERY 100.0
RESOURCES=100 i_reag_dragon_blood,1 i_crystal_puremagerobe

on=@create
tag.kullanim 50

on=@click
message @69,,1 [Charge:<eval <tag.kullanim>>]
message @,,1 <name>
return 1

on=@equip
if <eval <tag.kullanim>> <=0
	src.sys_error Talisman charge bitti ve yok oldu.
	remove
	return 1
endif
tag.kullanim --
src.sys_error Talisman Recharged.
src.nightsight 1

on=@unequip
src.nightsight 0

