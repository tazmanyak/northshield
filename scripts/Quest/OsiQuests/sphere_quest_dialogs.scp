[FUNCTION F_REDEEM]
IF (<KILLS> >= 5)
KILLS -= 5
ELIF (<KILLS> >= 4)
KILLS -= 4
ELIF (<KILLS> >= 3)
KILLS -= 3
ELIF (<KILLS> == 2)
KILLS -= 2
ELIF (<KILLS> == 1)
KILLS=0
ELSE
OBJ.SAY @33,3,1 Senin ruhun zaten temiz insanoglu!
RETURN 1
ENDIF
OBJ.SAY @33,3,1 Bagislandin <NAME>!





