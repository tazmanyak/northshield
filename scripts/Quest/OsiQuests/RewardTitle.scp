[FUNCTION DEATHTITLE]
IF (<DEATHS> >= 650)
RETURN I hate my life!
ELIF (<DEATHS> >= 450)
RETURN I must look both sides!
ELIF (<DEATHS> >= 300)
RETURN Sadomasoquist
ELIF (<DEATHS> >= 150)
RETURN Danger to all!
ELIF (<DEATHS> >= 90)
RETURN Danger to self!
ELIF (<DEATHS> >= 60)
RETURN Walking around with scissors!
ELIF (<DEATHS> >= 30)
RETURN Careless!
ELIF (<DEATHS> >= 10)
RETURN Please heal me!
ENDIF

[FUNCTION QUEST177REWARDTITLE]
