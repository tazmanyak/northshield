


[function spawnsil]
foritems 65000
if (<type>=t_spawn_char)
	if (<more>==0)
		remove
	endif
endif
endfor


[function evsil]
foritems 65000
	if (<type>=t_multi_custom)
		remove	
	endif
endfor


